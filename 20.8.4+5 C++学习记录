            //2.å…¨å±€å‡½æ•°é‡è½½è¿ç®—ç¬¦
            Operator operator+(Operator& p,Operator& p1)
            {
                Operator temp;
                temp.m_a = p.m_a + p1.m_a;
                temp.m_b = p.m_b + p1.m_b;
                return temp;
            }
            void test1()
            {
                Operator p1, p2;
                p1.m_a = 10;
                p2.m_a = 10;
                p1.m_b = 20;
                p2.m_b = 20;
                //1.æˆå‘˜å‡½æ•°é‡è½½è¿ç®—ç¬¦çš„æœ¬è´¨ï¼šOperator p3 = p1.operator+(p2);
