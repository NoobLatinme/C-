            class Person
            {
            public:
                Person(int age)
                {
                    m_Age = new int(age);
                }
                Person& operator=(Person& p)
                {
                    //å…ˆåˆ¤æ–­æ˜¯å¦æœ‰å±æ€§åœ¨å †åŒºï¼Œå¦‚æœæœ‰åˆ™å…ˆé‡Šæ”¾
                    if (m_Age != NULL)
                    {
                        delete m_Age;
                        m_Age = NULL;	
                    }
                    m_Age = new int(*p.m_Age);
                    return *this;
                }
                ~Person()
                {
                    if (m_Age != NULL)
                    {
                        delete m_Age;
                        m_Age = NULL;
                    }
                }
                int* m_Age;
            };
            void test1()
            {
                Person p1(18);
                Person p2(20);
                //Person p3 = p2 = p1;//p3çš„å¹´é¾„å†…å­˜å’Œp2çš„ä¸€æ ·
                Person p3(20);
                p3 = p1 = p2;
                cout << "p3 çš„å¹´é¾„ï¼š" << *p3.m_Age << endl;
            }
